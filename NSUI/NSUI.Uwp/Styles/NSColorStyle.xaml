<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="NSAccentBrush"
                             Color="#09BCBE" />
            <SolidColorBrush x:Key="NSBackgroundBrush"
                             Color="#EBEBEB" />
            <SolidColorBrush x:Key="NSForegroundBrush"
                             Color="#2D2D2D" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush x:Key="NSAccentBrush"
                             Color="#13BDF8" />
            <SolidColorBrush x:Key="NSBackgroundBrush"
                             Color="#2D2D2D" />
            <SolidColorBrush x:Key="NSForegroundBrush"
                             Color="#FEFEFE" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>